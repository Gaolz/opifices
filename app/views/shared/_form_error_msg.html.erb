<% if target.errors.any? %>
    <div class="container">
      <div class="alert alert-danger">
        <p><strong>发生以下错误：</strong></p>
        <ul>
          <% target.errors.full_messages.each do |msg| %>
              <li><%= msg %></li>
          <% end %>
        </ul>
      </div>
    </div>
<% end %>